    "W": {
      "callback": "player1.change_movement",
      "args": [ 0, -1, true ]
    },
    "S": {
      "callback": "player1.change_movement",
      "args": [ 0, 1, true ]
    },
    "A": {
      "callback": "player1.change_movement",
