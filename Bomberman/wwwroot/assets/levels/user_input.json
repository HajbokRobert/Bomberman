    },
    "A": {
      "callback": "player1.change_movement",
      "args": [ -1, 0, false ]
    },
    "D": {
      "callback": "player1.change_movement",
      "args": [ 1, 0, false ]
    },
    "I": {
      "callback": "player2.change_movement",
      "args": [ 0, -1, false ]
    },
    "K": {
      "callback": "player2.change_movement",
      "args": [ 0, 1, false ]
    },
    "J": {
      "callback": "player2.change_movement",
      "args": [ -1, 0, false ]
    },
    "L": {
      "callback": "player2.change_movement",
      "args": [ 1, 0, false ]
